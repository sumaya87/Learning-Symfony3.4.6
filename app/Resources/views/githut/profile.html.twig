

<img src="{{ avatar_url }}" class="img-thumbnail img-fluid" />
<h1>{{ name }}</h1>
<h1>{{ login }}</h1>

<hr />

<ul class="list-unstyled">
    {% for detail in details %}
        <li>{{ detail }}</li>
    {% endfor %}
    <li><a href="{{ blog }}">{{ blog }}</a></li>
</ul>


<ul class="list-inline">
    {% for key, value in social_data %}
        <li class="list-inline-item"><span class="social-metric-count">{{ value }}</span>
            <br />
            <span class="text-muted small">{{ key }}</span>
        </li>
    {% endfor %}
</ul>
